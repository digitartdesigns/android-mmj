<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id = "com.goldstarmmj.goldstarmmj"
        version = "1">
    <name>Gold Star Organic</name>
    <description>
        Web App by Digit Art Designs
    </description>
    <author href="http://digitartdesigns.com" email="support@digitartdesigns.com">
        Digit Art Designs Ltd. Team
    </author>
    <feature name="http://api.phonegap.com/1.0/device" />
	<preference name="splash-screen-duration" value="5000" />
    <preference name="orientation" value="default" />
    <preference name="fullscreen" value="false" />
	<gap:plugin name="org.apache.cordova.inappbrowser" />
	<icon src="images/icon-72.png" gap:platform="android" gap:density="hdpi" gap:role="default" />
	<gap:splash src="images/screen-portrait-md.png" gap:platform="android" gap:density="mdpi" />
	<gap:splash src="images/screen-portrait-lg.png" gap:platform="android" gap:density="ldpi" />
	<gap:splash src="images/screen-portrait-xh.png" gap:platform="android" gap:density="xhdpi" />
	<access origin="*" />
</widget>